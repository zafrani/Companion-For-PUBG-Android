<resources>
    <string name="app_name">pubgapp</string>
    <string name="title_activity_maps">Map</string>
    <string name="item_view">View: </string>

    <string name="item_view_linear_description">Linear item display.</string>
    <string name="item_view_grid_description">Grid item display.</string>

    <string name="row_item_ammo">Ammo: %1$s</string>
    <string name="row_item_magazine">Magazine: %1$.0f</string>
    <string name="row_item_capacity">Capacity: %1$.0f</string>
    <string name="row_item_range">Range: %1$f</string>
    <string name="row_item_stability">Stability: %1$.0f</string>
    <string name="row_item_rate">Rate: %1$.0f</string>
    <string name="row_item_damage">Damage: %1$s</string>

    <!--
        You will need to provide a Google Maps API key to run this.

        The way I run this is by creating a file called 'local-strings.xml'. The file is NEVER
        added to Git. Inside of it I created a string resource called 'local_maps_api_key', it
        contains the API key from https://console.developers.google.com/apis that will be used.
     -->
    <string name="maps_api_key">@string/local_maps_api_key</string>
</resources>
